{
    "name": "gitbook/framework",
    "keywords": [
        "Framework",
        "PHP-GB",
        "phps-gitbook-framework",
        "web-framework"
    ],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Andr√© Santos",
            "role": "Developer"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform": {
            "php": "8.2"
        },
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "vlucas/phpdotenv": "^5.6",
        "guzzlehttp/guzzle": "^7.9",
        "hhxsv5/php-sse": "~2.0",
        "nesbot/carbon": "^3.9",
        "cloudinary/cloudinary_php": "^2"
    },

    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    }
}
